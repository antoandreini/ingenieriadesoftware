<table class="table">

<% @gauchada=Gauchada.all %>
  <% @gauchada.each do |gauchada| %>
	<tr>
      <td ALIGN=CENTER> <h3>  <%=  link_to gauchada.titulo, gauchada %></h3	> </td>
      <td ALIGN=CENTER> <%=image_tag gauchada.imagen, size:"100x100" %> </td>
      <td ALIGN=CENTER> <h3>  <%= gauchada.ubicacion %> </h3> </td>
    </tr>
<% end %>
